  "repository": {
    "type": "git",
    "url": "https://github.com/saurshaz/docker-react-redux-webpack2-starterkit"
  },
  "bugs": {
    "url": "https://github.com/saurshaz/docker-react-redux-webpack2-starterkit/issues"
  },
  "scripts": {
    "starts": "node server.js",
    "testservers": "npm run build && node testserver.js",
    "builds": "rm -rf dist && cross-env NODE_ENV=production webpack -p && cp -R ./src/config ./dist",
    "test": "cross-env BABEL_ENV=test mocha --compilers js:babel-register --require ./test/testHelper.js --recursive ./src/**/*.spec.js",
    "test:unit": "cross-env BABEL_ENV=test mocha --compilers js:babel-register --require ./test/testHelper.js --recursive ./test/**/*.spec.js",
    "test:coverage": "cross-env BABEL_ENV=test nyc --require babel-core/register mocha --require ./test/testHelper.js --recursive ./test/**/*.spec.js",
    "createLocaleFiles": "./bin/extract_lang.sh",
    "convertPOtoJson": "./bin/po2json.sh",
    "clean": "rimraf dist/*",
    "copy": "copyfiles -f ./src/index.html ./dist && copyfiles -u 1 \"./src/static/**\" ./dist",
    "build": "npm run clean && npm run copy && webpack --progress --bail --env dist -p",
    "lint": "eslint ./src",
    "client": "./node_modules/.bin/webpack-dev-server --progress --host=localhost --hot --config=./webpack.dev.config.js",
    "posttest": "npm run lint",
    "serve:dev": "./node_modules/.bin/webpack-dev-server  --hot --host localhost --port 8080 --env dev",
    "serve:dist": "./node_modules/.bin/webpack-dev-server  --hot --env dist -p --progress",
    "start": "npm run serve:dev"
  },
